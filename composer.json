{
  "name": "thormaweb/laravel-admin-theme",
  "description":
    "Beautifully designed administration panel for Laravel with out of the box Users roles management for scaffolding new projects.",
  "type": "library",
  "keywords": [
    "thormaweb",
    "laravel",
    "admin",
    "dashboard",
    "backend",
    "user roles",
    "roles"
  ],
  "homepage": "https://github.com/thormaweb/laravel-admin-theme",
  "license": "MIT",
  "authors": [
    {
      "name": "emilianotisato",
      "email": "emiliano@thormaweb.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "~5.6.0|~5.7.0",
    "illuminate/filesystem": "~5.6.0|~5.7.0",
    "spatie/laravel-medialibrary": "7.5.*",
    "spatie/laravel-permission": "2.23.*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0",
    "phpunit/phpunit": "^5.7|6.2|^7.0"
  },
  "autoload": {
    "psr-4": {
      "ThormaWeb\\AdminTheme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ThormaWeb\\AdminTheme\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": ["ThormaWeb\\AdminTheme\\AdminThemeServiceProvider"]
    },
    "aliases": {
            "AdminTheme": "ThormaWeb\\AdminTheme\\AdminThemeFacade"
        }
  }
}
